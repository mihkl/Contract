#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/falin/Documents/VsCode projects/itb2203_2024_lepingud-6/node_modules/.pnpm/changelogen@0.5.7/node_modules/changelogen/dist/node_modules:/mnt/c/Users/falin/Documents/VsCode projects/itb2203_2024_lepingud-6/node_modules/.pnpm/changelogen@0.5.7/node_modules/changelogen/node_modules:/mnt/c/Users/falin/Documents/VsCode projects/itb2203_2024_lepingud-6/node_modules/.pnpm/changelogen@0.5.7/node_modules:/mnt/c/Users/falin/Documents/VsCode projects/itb2203_2024_lepingud-6/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/falin/Documents/VsCode projects/itb2203_2024_lepingud-6/node_modules/.pnpm/changelogen@0.5.7/node_modules/changelogen/dist/node_modules:/mnt/c/Users/falin/Documents/VsCode projects/itb2203_2024_lepingud-6/node_modules/.pnpm/changelogen@0.5.7/node_modules/changelogen/node_modules:/mnt/c/Users/falin/Documents/VsCode projects/itb2203_2024_lepingud-6/node_modules/.pnpm/changelogen@0.5.7/node_modules:/mnt/c/Users/falin/Documents/VsCode projects/itb2203_2024_lepingud-6/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../changelogen/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../changelogen/dist/cli.mjs" "$@"
fi
